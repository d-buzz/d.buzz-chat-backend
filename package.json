{
  "name": "sting",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "buildlib": "tsc -p \"libs/stlib/tsconfig.web.json\" && browserify \"dist/web/stlib/stlib.js\" -o \"dist/web/bundle/stlib.js\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start backend",
    "start:watch": "nest start backend --watch",
    "start:dev": "nest start backend --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm install && nest start backend",
    "startts": "ts-node apps/backend/src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/backend/test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@hiveio/dhive": "^1.2.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.7",
    "@nestjs/platform-ws": "^8.4.7",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/websockets": "^8.4.7",
    "axios": "^0.27.2",
    "better-sqlite3": "^7.5.3",
    "cache-manager": "^4.0.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.5.3",
    "sqlite3": "^5.1.2",
    "typeorm": "0.3.6",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "browserify": "^17.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/stlib(|/.*)$": "<rootDir>/libs/stlib/src/$1"
    }
  }
}
